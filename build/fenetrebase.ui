    <string notr="true">
/* üåë FOND GLOBAL */
QWidget {
    background-color: #1A1A1A;  /* Fond sombre pour limiter la fatigue visuelle */
    color: #FFFFFF;             /* Texte blanc pour un bon contraste */
    font-family: Arial, sans-serif;
    font-size: 16px;
}

/* üîò BOUTONS (QPushButton) */
QPushButton {
    background-color: #900000;  /* Rouge intense */
    color: white;
    font-size: 18px;
    font-weight: bold;
    border: 2px solid #D00000;
    border-radius: 10px;
    padding: 10px;
    min-width: 120px;
    min-height: 40px;
}
QPushButton:hover {
    background-color: #B00000;
    border: 2px solid #FF0000;
}
QPushButton:pressed {
    background-color: #700000;
    border: 2px solid #FF0000;
    padding-left: 12px;
    padding-top: 12px;
}

/* üéöÔ∏è SLIDER VERTICAL AVEC EFFET LUMINEUX */
QSlider::groove:vertical {
    border: 1px solid #900000;
    background: #1A1A1A;          /* Fond sombre */
    width: 8px;
    border-radius: 4px;
}

QSlider::handle:vertical {
    background: qradialgradient(
        cx:0.5, cy:0.5, radius:0.7,
        fx:0.5, fy:0.5,
        stop:0 #FF0000,
        stop:1 #900000
    );
    border: 2px solid #FF5555;
    height: 20px;
    width: 20px;
    margin: 0 -6px;
    border-radius: 10px;
}

/* Partie remplie (en bas vers le curseur) */
QSlider::sub-page:vertical {
    background: qlineargradient(
        x1:0, y1:1, x2:0, y2:0,
        stop:0 #FF0000,
        stop:0.5 #FF5500,
        stop:1 #FFAA00
    );
    border: 1px solid #FF5500;
    border-radius: 4px;
}

/* Partie vide (au-dessus du curseur) */
QSlider::add-page:vertical {
    background: #1A1A1A;
    border: 1px solid #900000;
    border-radius: 4px;
}

/* üìã MENUS D√âROULANTS (QComboBox) */
QComboBox {
    background-color: #300000;
    color: white;
    font-size: 16px;
    border: 2px solid #900000;
    border-radius: 8px;
    padding: 5px;
}
QComboBox:hover {
    background-color: #500000;
}
QComboBox QAbstractItemView {
    background-color: #300000;
    color: white;
    selection-background-color: #D00000;
}

/* üè∑Ô∏è TEXTES ET √âTIQUETTES (QLabel) */
QLabel {
    color: #D00000;
    font-size: 18px;
    font-weight: bold;
}

/* ‚å®Ô∏è CHAMPS DE TEXTE (QLineEdit) */
QLineEdit {
    background-color: #222;
    color: white;
    border: 2px solid #900000;
    border-radius: 5px;
    padding: 5px;
}
QLineEdit:focus {
    border: 2px solid #FF0000;
}</string>
