          <property name="styleSheet">
           <string notr="true">/* üåë FOND GLOBAL */
QWidget {
    background-color: #1A1A1A;  /* Fond sombre pour limiter la fatigue visuelle */
    color: #FFFFFF;             /* Texte blanc pour un bon contraste */
    font-family: Arial, sans-serif;
    font-size: 16px;
}

/* üîò BOUTONS (QPushButton) */
QPushButton {
    background-color: #900000;  /* Rouge intense */
    color: white;
    font-size: 18px;
    font-weight: bold;
    border: 2px solid #D00000;
    border-radius: 10px;
    padding: 10px;
    min-width: 120px;
    min-height: 40px;
}
QPushButton:hover {
    background-color: #B00000;
    border: 2px solid #FF0000;
}
QPushButton:pressed {
    background-color: #700000;
    border: 2px solid #FF0000;
    padding-left: 12px;
    padding-top: 12px;
}

/* üéöÔ∏è SLIDERS (QSlider) */
QSlider::groove:horizontal {
    border: 1px solid #D00000;
    background: #300000;
    height: 8px;
    border-radius: 4px;
}
QSlider::handle:horizontal {
    background: #FF0000;
    border: 2px solid #900000;
    width: 20px;
    height: 20px;
    margin: -6px 0;
    border-radius: 10px;
}
QSlider::sub-page:horizontal {
    background: #D00000;
    border: 1px solid #900000;
}

/* üìã MENUS D√âROULANTS (QComboBox) */
QComboBox {
    background-color: #300000;
    color: white;
    font-size: 16px;
    border: 2px solid #900000;
    border-radius: 8px;
    padding: 5px;
}
QComboBox:hover {
    background-color: #500000;
}
QComboBox QAbstractItemView {
    background-color: #300000;
    color: white;
    selection-background-color: #D00000;
}

/* üè∑Ô∏è TEXTES ET √âTIQUETTES (QLabel) */
QLabel {
    color: #D00000;
    font-size: 18px;
    font-weight: bold;
}

/* ‚å®Ô∏è CHAMPS DE TEXTE (QLineEdit) */
QLineEdit {
    background-color: #222;
    color: white;
    border: 2px solid #900000;
    border-radius: 5px;
    padding: 5px;
}
QLineEdit:focus {
    border: 2px solid #FF0000;
}</string>
